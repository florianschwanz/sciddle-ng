{
  "name": "amphibian7",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "ng test --reporters junit --code-coverage",
    "lint": "ng lint",
    "lint-junit": "./node_modules/.bin/tslint -c ./tslint.json \"src/**/*.ts\" --format junit --out reports/lint-results.xml",
    "compodoc": "compodoc -p tsconfig.json --output reports/documentation/",
    "e2e": "ng e2e",
    "electron-build": "ng build --base-href ./ && cd ./src/electron && copyfiles ./* ./../../dist/",
    "electron-build-prod": "ng build --prod --base-href ./ && cd ./src/electron && copyfiles ./* ./../../dist/",
    "electron-start": "npm run electron-build && electron ./dist/.",
    "electron-linux": "npm run electron-build && electron-packager ./dist %npm_package_name% --icon=src/favicon.ico --platform=linux --out=out --prune=true --overwrite",
    "electron-linux-prod": "npm run electron-build-prod && electron-packager ./dist %npm_package_name% --icon=src/favicon.ico --platform=linux --out=out --prune=true --overwrite",
    "electron-macos": "npm run electron-build && electron-packager ./dist %npm_package_name% --icon=src/favicon.ico --platform=darwin --out=out --overwrite",
    "electron-macos-prod": "npm run electron-build-prod && electron-packager ./dist %npm_package_name% --icon=src/favicon.ico --platform=darwin --out=out --overwrite",
    "electron-windows": "npm run electron-build && electron-packager ./dist %npm_package_name% --icon=src/favicon.ico --platform=win32 --out=out --overwrite",
    "electron-windows-prod": "npm run electron-build-prod && electron-packager ./dist %npm_package_name% --icon=src/favicon.ico --platform=win32 --out=out --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/cdk": "^7.3.1",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.1",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "~7.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.40",
    "codelyzer": "~4.5.0",
    "compodoc": "0.0.41",
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.4"
  }
}
