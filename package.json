{
  "name": "sciddle",
  "version": "1.10.0",
  "authorCode": "Florian Schwanz",
  "authorCodeUrl": "https://github.com/florianschwanz",
  "authorOriginal": "Anne Braun, Anne Dippel, Thomas Klose, Britta Kussin, Dennis Rätzel",
  "authorContent": "Dennis Rätzel, Kiri Mochrie, Jana Mochrie und Julia Mochrie",
  "authorGraphics": "Britta Kussin",
  "authorGraphicsUrl": "https://www.britta-kussin.de/start-sciddle-the-science-riddle",
  "authorScientificSupervision": "Maria Zeitz (Potsdam-Institut für Klimafolgenforschung)",
  "githubUrl": "https://github.com/sciddle/sciddle-ng",
  "licenseCode": "AGPLv3",
  "licenseContent": "CC BY 4.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf ./dist",
    "start": "ng serve --configuration=de-regular",
    "start-de": "ng serve --configuration=de-regular",
    "start-en": "ng serve --configuration=en-regular",
    "build": "ng build",
    "build:ci": "npm run clean && npm run build",
    "build-de-regular-prod": "ng build --deleteOutputPath=true --configuration=de-regular-production --outputPath=dist/sciddle-riddle-de",
    "build-en-regular-prod": "ng build --deleteOutputPath=true --configuration=en-regular-production --outputPath=dist/sciddle-riddle-en",
    "build-de-s4f-prod": "ng build --deleteOutputPath=true --configuration=de-s4f-production --outputPath=dist/sciddle-s4f-de",
    "test": "ng test --reporters junit --code-coverage",
    "lint": "ng lint",
    "lint-junit": "./node_modules/.bin/tslint -c ./tslint.json \"src/**/*.ts\" --format junit --out reports/lint-results.xml",
    "compodoc": "compodoc src/app -p tsconfig.json --output reports/documentation/",
    "e2e": "ng e2e",
    "int:extract": "ng xi18n --output-path ./locale",
    "electron-build": "npm run build -- --base-href ./ && cd ./src/electron && copyfiles ./* ./../../dist/",
    "electron-build-de-regular-prod": "npm run build-de-regular-prod -- --base-href ./ && cd ./src/electron && copyfiles ./* ./../../dist/",
    "electron-start": "npm run electron-build && electron ./dist/.",
    "electron-linux": "npm run electron-build && electron-packager ./dist ${npm_package_name} --icon=src/favicon.ico --platform=linux --out=out --prune=true --overwrite",
    "electron-linux-prod": "npm run electron-build-de-regular-prod && electron-packager ./dist ${npm_package_name} --icon=src/favicon.ico --platform=linux --out=out --prune=true --overwrite",
    "electron-macos": "npm run electron-build && electron-packager ./dist ${npm_package_name} --icon=src/favicon.ico --platform=darwin --out=out --overwrite",
    "electron-macos-prod": "npm run electron-build-de-regular-prod && electron-packager ./dist ${npm_package_name} --icon=src/favicon.ico --platform=darwin --out=out --overwrite",
    "electron-windows": "npm run electron-build && electron-packager ./dist ${npm_package_name} --icon=src/favicon.ico --platform=win32 --out=out --overwrite",
    "electron-windows-prod": "npm run electron-build-de-regular-prod && electron-packager ./dist ${npm_package_name} --icon=src/favicon.ico --platform=win32 --out=out --overwrite",
    "cordova-clean": "cd ./cordova && cordova clean",
    "cordova-android-prepare": "cd ./cordova && cordova platform remove android && cordova platform add android",
    "cordova-android": "cd ./cordova && cordova build android --debug",
    "cordova-android-prod": "cd ./cordova && cordova build android --release",
    "cordova-android-start": "npm run cordova-android && cd ./cordova && cordova emulate android",
    "release-web": "mkdirp ./release && npm run build-de-regular-prod && cd dist/sciddle-riddle && bestzip ./../../release/${npm_package_name}-web-${npm_package_version}.zip  ./*",
    "release-web-s4f": "mkdirp ./release && npm run build-de-s4f-prod && cd dist/sciddle-s4f && bestzip ./../../release/${npm_package_name}-s4f-web-${npm_package_version}.zip  ./*",
    "release-only-web-de": "mkdirp ./release && cd dist/sciddle-riddle-de && bestzip ./../../release/${npm_package_name}-web-de-${npm_package_version}.zip  ./*",
    "release-only-web-en": "mkdirp ./release && cd dist/sciddle-riddle-en && bestzip ./../../release/${npm_package_name}-web-en-${npm_package_version}.zip  ./*",
    "release-only-web-s4f": "mkdirp ./release && cd dist/sciddle-s4f-de && bestzip ./../../release/${npm_package_name}-s4f-web-${npm_package_version}.zip  ./*",
    "release-linux": "mkdirp ./release && npm run electron-linux-prod && cd ./out && bestzip ./../../release/${npm_package_name}-linux-x64-${npm_package_version}.zip ./${npm_package_name}-linux-x64/*",
    "release-macos": "mkdirp ./release && npm run electron-macos-prod && cd ./out && bestzip ./../../release/${npm_package_name}-macos-x64-${npm_package_version}.zip ./${npm_package_name}-darwin-x64/*",
    "release-windows": "mkdirp ./release && npm run electron-windows-prod && cd ./out && bestzip ./../../release/${npm_package_name}-win32-x64-${npm_package_version}.zip ./${npm_package_name}-win32-x64/*",
    "release-android": "mkdirp ./release && npm run cordova-android-prod && cd ./cordova/platforms/android/build/outputs/apk && bestzip ./../../../../../../../release/${npm_package_name}-android-${npm_package_version}.zip ./android-release-unsigned.apk",
    "release-notes": "mkdirp ./release && markdown ./src/releasenotes/release-notes-${npm_package_version}.md > release/release-notes-${npm_package_version}.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/material": "^12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@angular/service-worker": "~12.1.1",
    "angular2-swing": "^0.14.0",
    "core-js": "^2.6.12",
    "highlight.js": "^9.18.5",
    "markdown-it": "^8.4.2",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "pouchdb-upsert": "^2.2.0",
    "rxjs": "^6.6.7",
    "tslib": "^1.14.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular/cli": "^12.1.1",
    "@angular/compiler-cli": "^12.1.1",
    "@angular/language-service": "~12.1.1",
    "@angular/localize": "^12.1.1",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "^2.8.17",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^8.10.66",
    "bestzip": "^2.2.0",
    "codelyzer": "^5.2.2",
    "cordova": "^10.0.0",
    "electron": "^13.1.5",
    "electron-builder": "^22.11.8",
    "electron-packager": "^15.2.0",
    "git": "^0.1.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "markdown-to-html": "0.0.13",
    "mkdirp": "^1.0.4",
    "protractor": "^7.0.0",
    "replace": "^1.2.1",
    "ts-node": "~7.0.0",
    "tslint": "^5.20.1",
    "typescript": "^4.3.5"
  }
}
